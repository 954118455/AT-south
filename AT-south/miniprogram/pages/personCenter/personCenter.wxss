/* pages/personCenter/personCenter.wxss */
page {
  background: white;
}

.personal {
  width: 750rpx;
  height: 386rpx;
  display: flex;
  flex-flow: column;
}

.userinfo {
  margin-top: 0rpx;
  margin-left: 40rpx;
  width: 670rpx;
  height: 186rpx;
  border: none;
}

.auth {
  width: auto;
  background-color: #54c38d;
  color: white;
}

.b1 {
  width: 670rpx;
  height: 186rpx;
  background-color: white;
  display: flex;
  flex-flow: column;
}

.name {
  margin-top: 28rpx;
  width: 670rpx;
  height: 48rpx;
  display: flex;
  flex-flow: row;
}

.nickName {
  margin-top: 12rpx;
  height: 36rpx;
  font-size: 18px;
  font-family: PingFang SC, PingFang SC-Semibold;
  font-weight: 600;
  text-align: LEFT;
  color: #000000;
  line-height: 36rpx;
}

.renzheng {
  margin: 12rpx 0rpx 4rpx 24rpx;
  width: 104rpx;
  height: 32rpx;
  border-radius: 18rpx;
  border: 2rpx solid #54c38d;
  align-items: center;
  justify-content: center;
  font-size: 10px;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: center;
  color: #54c38d;
}

.name-img {
  margin-left: 24rpx;
  width: 48rpx;
  height: 48rpx;
}

.date {
  margin-top: 32rpx;
  width: auto;
  height: 38rpx;
  display: flex;
  flex-flow: row;
}

.college {
  width: auto;
  padding: 0rpx 18rpx;
  height: 38rpx;
  opacity: 1;
  border: 2rpx solid rgba(0, 0, 0, 0.10);
  border-radius: 18rpx;
  align-items: center;
  justify-content: center;
}

.id {
  margin-left: 32rpx;
  width: auto;
  height: 38rpx;
  padding: 0rpx 18rpx;
  opacity: 1;
  border: 2rpx solid rgba(0, 0, 0, 0.10);
  border-radius: 18rpx;
  align-items: center;
  justify-content: center;
}

.b1-text {
  font-size: 10px;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: center;
  color: rgba(0, 0, 0, 0.38);
  letter-spacing: 0px;
}

.head {
  position: absolute;
  top: 18rpx;
  right: 56rpx;
  width: 128rpx;
  height: 128rpx;
  border-radius: 56rpx;
}

/*顶部积分*/
.nav-integral {
  position: relative;
  margin-left: 40rpx;
  width: 670rpx;
  height: 200rpx;
}

.nav-integral-text {
  position: absolute;
  top: 82rpx;
  left: 302rpx;
  width: 40px;
  height: 10px;
  font-size: 10px;
  font-family: PingFang SC, PingFang SC-Regular;
  font-weight: 400;
  text-align: CENTER;
  color: #54c38d;
  line-height: 10px;
  z-index: 10;
}
.nav-integral-img {
  float: left;
  width: 100%;
  height: 100%;
}

/*小图标*/
.plate {
  margin-left: 40rpx;
